    <%= form.label :latLocation %>
    <%= form.text_field :latLocation, id: :city_latLocation %>
  </div>

  <div class="field">
    <%= form.label :longLocation %>
    <%= form.text_field :longLocation, id: :city_longLocation %>
  </div>

  <div class="field">
    <%= form.label :timezone %>
